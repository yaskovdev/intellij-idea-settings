<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'Ctrl+K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reload Page">
        <Item>
          <IdeaActionID>Console.TableResult.Reload</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Reload Page</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>